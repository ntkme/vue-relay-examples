{
  "env": {
    "node": {
      "presets": [
        ["env", {
          "targets": {
            "node": "current"
          }
        }],
        "stage-2"
      ],
      "plugins": ["transform-runtime"]
    },
  },
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"],
        "node": "current"
      }
    }],
    "stage-2"
  ],
  "plugins": ["relay", "transform-vue-jsx", "transform-runtime"]
}
