{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production babel-node index.js",
    "serve": "babel-node index.js",
    "build": "npm run relay && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "relay": "npm run relay:update-schema && npm run relay:compile",
    "relay:compile": "relay-compiler --src ./src --schema ./data/schema.graphql",
    "relay:update-schema": "babel-node ./build/update-schema"
  },
  "dependencies": {
    "@babel/node": "^7.5.5",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "graphql-relay": "^0.6.0",
    "todomvc-app-css": "^2.2.0",
    "todomvc-common": "^1.0.5",
    "vue": "^2.6.10",
    "vue-relay": "^5.0.0",
    "vue-router": "^3.1.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-plugin-relay": "^5.0.0",
    "relay-compiler": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
